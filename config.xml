<?xml version='1.0' encoding='utf-8'?>
<widget id="com.modernpublicschool.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Modern Public School</name>
  <description>Wrapped web app for Modern Public School.</description>
  <author email="you@example.com" href="http://example.com">You</author>
  <content src="index.html" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <preference name="SplashScreen" value="screen" />
  <preference name="AndroidPersistentFileLocation" value="Internal" />
</widget>
